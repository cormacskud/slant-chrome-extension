{
    "name": "Slant",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
        "webRequest", "tabs", "scripting"
    ],
    "host_permissions": [
        "https://newsapi.org/*",
        "https://maxcdn.bootstrapcdn.com/*",
        "https://ajax.googleapis.com/*"
    ],
    "action": {
        "default_title": "What's the SLANT?"
    },
    "icons": { "16": "slant-icon-16.png",
        "48": "slant-icon-48.png",
       "128": "slant-icon-128.png"}
  }